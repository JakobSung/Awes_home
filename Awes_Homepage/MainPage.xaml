<Page x:Class="Awes_Homepage.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Awes_Homepage"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d"
    Foreground="{DynamicResource Theme_TextBrush}"
    FontFamily="Anton, Arial, Sans-Serif">

    <Grid Background="{DynamicResource Theme_BackgroundBrush}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Awes" FontSize="123" Margin="32,32,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Page>
